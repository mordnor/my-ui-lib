    "eslint-plugin-tailwindcss": "^3.18.2",
    "gh-pages": "^6.1.1",
    "happy-dom": "^15.11.7",
    "postcss": "^8.5.6",
    "prettier": "3.1.1",
    "style-dictionary": "^5.1.0",
    "tailwindcss": "^3.4.17",
    "typescript": "^5.9.2",
    "vite": "^5.4.20",
    "vite-tsconfig-paths": "^4.3.2",
    "vitest": "^1.6.1"
  }
